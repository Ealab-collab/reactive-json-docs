{
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 120,
  "endOfLine": "lf",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": ["*.yml", "*.yaml", "*.json", "*.md"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
